	<xs:complexType name="learningStandardAssociationType">
		<xs:annotation>
			<xs:documentation>This object associates a learning standard with particular learning resources.  </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gAssociationType">
				<xs:sequence>
					<xs:element name="refURI" type="xs:anyURI" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>A URI that establishes uniqueness.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="learningStandardRefId" type="gRefIdPointerType">
						<xs:annotation>
							<xs:documentation>The RefId of a learning standard. The sif_Object attribute value should be either LearningStandardItem or LearningStandardDocument.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="targetObjectList">
						<xs:annotation>
							<xs:documentation>List of target objects for the learning standard linkage. </xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="targetObject" minOccurs="0" maxOccurs="unbounded">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="targetObjectRefId" type="gGenericRefIdPointerType">
												<xs:annotation>
													<xs:documentation>The RefId for the LearningResource or AssessmentItem. The sif_Object attribute value must be LearningResource or AssessmentItem</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="associationStrength" type="xs:decimal" nillable="true" minOccurs="0">
												<xs:annotation>
													<xs:documentation>A number greater than 0 and less than or equal to 1. This indicates the strength of the association between the one or more Learning Standards and this target object. </xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="associationType" type="xs:token" nillable="true" minOccurs="0">
												<xs:annotation>
													<xs:documentation>The type of relationship or cardinality being modeled, e.g., predecessor, prerequisite, unidirectional, nondirectional, is a component of, etc. </xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="startDate" type="xs:date">
						<xs:annotation>
							<xs:documentation>Start date of the learning standard association.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="endDate" type="xs:date" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>End date of the learning standard association</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="authoritativeSource" type="xs:normalizedString" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Body or organization creating this association. </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="associationCreator" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="creatorName" type="xs:normalizedString" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Name of the Creator of the relationship. </xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="creatorRefId" type="gGenericRefIdPointerType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>RefId of SIF object representing the creator if such exists.</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="creationDate" type="xs:date" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Creation date of the relationship. </xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
