	<xs:complexType name="seaType">
		<xs:annotation>
			<xs:appinfo>
				<elementName>SEA</elementName>
				<events>yes</events>
				<isSIFObject>yes</isSIFObject>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>This object is designed to facilitate the transfer of State Education Agency (SEA) directory information to the Department of Education (USED) through the Education Data Exchange Network (EDEN).</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gEducationOrganizationType">
				<xs:sequence>
					<xs:element name="localId" type="gLocalIdType">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>M</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>The locally-assigned identifier for this SEA.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ncesId" type="gExternalIdType" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>The NCES-assigned identifier for this SEA.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="seaName" type="xs:normalizedString">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>M</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>The full legally accepted name of the SEA.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="seaURL" type="xs:anyURI" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>URL for the SEA.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ccssoContact" type="xs:token" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>Chief State School Officer contact information.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="seaContactList" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>A list of contacts for the SEA.</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="seaContact" type="gContactPersonType" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:appinfo>
											<sifChar>OR</sifChar>
											<cedsId/>
											<cedsURL/>
										</xs:appinfo>
										<xs:documentation/>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="phoneNumberList" type="gPhoneNumberList" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>The SEA's phone number(s).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="addressList" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>The SEA's address(es).</xs:documentation>
