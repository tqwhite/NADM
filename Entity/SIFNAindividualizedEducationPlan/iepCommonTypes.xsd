			<xs:element name="staffRefId" type="iepCommonStaffRefIdPointerType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Pointer to xStaff when service provider is a staff member </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="role" minOccurs="0">
