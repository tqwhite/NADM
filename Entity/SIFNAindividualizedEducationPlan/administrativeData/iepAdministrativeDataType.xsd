<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema
	xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<!-- start redundant declaration  for SIF tool analysis -->
	<xs:include schemaLocation="../../../SIFglobal.xsd" />
	<xs:include schemaLocation="../../../Composite/SIFNAxPressCommonTypes.xsd" />
	<xs:include schemaLocation="../../../Composite/SIFNAxPress.xsd" />
	<xs:include schemaLocation="../iepCommonTypes.xsd" />
	<xs:include schemaLocation="../../../Entity/SIFNAEntityObjects.xsd" />
	<!-- end redundant declaration for SIF tool analysis -->
	<xs:include schemaLocation="iepAdministrativeDataCostSharingType.xsd" />
	<xs:include schemaLocation="iepTransmitPermissionListType.xsd" />
	<xs:include schemaLocation="../../../Composite/SIFNAxPress.xsd" />
	<!-- local parent type -->
	<xs:complexType name="iepAdministrativeDataType">
		<xs:annotation>
			<xs:appinfo>
				<elementName>Special Education</elementName>
				<sifChar>O</sifChar>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation>
				Administrative and demographic data supporting Special Education students
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gSIF_BaseType">
				<xs:sequence>
					<xs:element name="studentRefId" type="iepCommonStudentRefIdPointerType" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<elementName>Student Reference Identifier</elementName>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL></cedsURL>
							</xs:appinfo>
							<xs:documentation>This is the reference identifier of the student who is the subject of the IEP.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="schoolRefId" type="iepCommonSchoolRefIdPointerType" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<elementName>School Reference Identifier</elementName>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL></cedsURL>
							</xs:appinfo>
							<xs:documentation>This is the reference identifier of the school that administers the IEP (especially for cases where the student is attending a different school).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="planPdfRefId" type="iepCommonPdfRefIdPointerType" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<elementName>Plan PDF Identifier</elementName>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL></cedsURL>
							</xs:appinfo>
							<xs:documentation>This is the reference identifier of the PDF corresponding to this plan.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element type="xs:date" name="reevaluationDueDate" minOccurs="0">
						<xs:annotation>
							<xs:appinfo>
								<elementName>
									Date of next evaluation
								</elementName>
								<sifChar>O</sifChar>
								<cedsId/>
								<cedsURL/>
							</xs:appinfo>
							<xs:documentation>The date that the next evaluation is due based on the three year cycle.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element type="iepAdministrativeDataCostSharingType" name="costSharing" minOccurs="0" />
					<xs:element type="iepTransmitPermissionListType" name="transferPermissionList" minOccurs="0" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
</xs:schema>